<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >

<generatorConfiguration>
<properties url="file:E:/workspace/huayi-commerce/huayi-commerce-gen/src/main/resources/mbg.properties"/>
<classPathEntry location="${mbg.jar.path}"/>
<context id="context1" defaultModelType="flat">

    <commentGenerator>
        <property name="suppressAllComments" value="true" />
    </commentGenerator>

    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                    connectionURL="jdbc:mysql://127.0.0.1:3306/huayi-commerce?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=false&amp;nullNamePatternMatchesAll=true"
                    userId="root"
                    password="123456"/>

    <javaModelGenerator targetPackage="com.commerce.huayi.website.entity.db"
                        targetProject="${target.module.sdk.base}/src/main/java">
        <property name="mergeable" value="false"/>
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mapper"
                     targetProject="${target.module.sdk.base}/src/main/resources"/>

    <javaClientGenerator targetPackage="com.commerce.huayi.website.mapper"
                         targetProject="${target.module.sdk.base}/src/main/java"
                         type="XMLMAPPER"/>

    <table schema="huayi-commerce" tableName="tb_user" domainObjectName="User"></table>

</context>
</generatorConfiguration>